{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "________________ development ________________": "",
    "start": "yarn gen:types && react-scripts start",
    "start:staging": "env-cmd .env.staging react-scripts start",
    "start:prod": "env-cmd .env.prod react-scripts start",
    "db": "yarn hasura console",
    "hasura": "env-cmd .env hasura --project hasura",
    "test": "react-scripts test",
    "serve": "serve -s build",
    "upgr": "yarn upgrade-interactive --latest",
    "up": "docker-compose up -d",
    "stop": "docker-compose stop",
    "________________ deployment ________________": "",
    "build:staging": "env-cmd .env.staging npm run build",
    "build:prod": "env-cmd .env.prod npm run build",
    "migrate:staging": "yarn hasura migrate apply --endpoint http://another-graphql-instance.herokuapp.com",
    "________________ implementation details ________________": "",
    "gen:types": "(mkdirp hasura/gen && gq http://localhost:8080/v1/graphql -H \"X-Hasura-Admin-Secret: secret\" --introspect > hasura/gen/schema.graphql && graphql-codegen --config ./hasura/codegen.yml) || echo WARN: could not regenerate schemas. Maybe hasura is not started?",
    "build": "react-scripts build",
    "build-snapshot": "yarn build && react-snapshot",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "4.9.0",
    "@material-ui/icons": "4.5.1",
    "apollo-cache": "^1.3.4",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-retry": "^2.2.15",
    "apollo-link-ws": "^1.0.19",
    "apollo-utilities": "^1.3.3",
    "axios": "0.19.2",
    "connected-react-router": "6.6.1",
    "formik": "2.1.2",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "history": "4.10.1",
    "immutable": "4.0.0-rc.12",
    "jwt-decode": "^2.2.0",
    "react": "16.12.0",
    "react-apollo": "^3.1.3",
    "react-app-polyfill": "1.0.5",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-immutable": "4.0.0",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typeface-roboto": "0.0.75",
    "yup": "^0.28.0",
    "zen-observable-ts": "^0.8.20"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@graphql-codegen/cli": "^1.11.2",
    "@graphql-codegen/typescript": "^1.11.2",
    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "8.0.4",
    "@types/history": "4.7.4",
    "@types/jest": "24.9.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "13.5.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@types/redux-immutable": "4.0.1",
    "@types/webpack-env": "1.15.0",
    "@types/yup": "^0.26.27",
    "cross-env": "^6.0.3",
    "env-cmd": "8.0.2",
    "graphqurl": "^0.3.3",
    "hasura-cli": "^1.0.0",
    "http-proxy-middleware": "^0.20.0",
    "jest-when": "^2.7.0",
    "mkdirp": "^0.5.1",
    "prettier": "1.19.1",
    "react-scripts": "3.3.0",
    "react-snapshot": "1.3.0",
    "serve": "11.3.0",
    "tslint": "6.0.0",
    "tslint-config-blvd": "1.2.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.5"
  },
  "homepage": "/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": true
  }
}

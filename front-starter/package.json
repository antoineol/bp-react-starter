{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "________________ development ________________": "",
    "start": "cross-env PORT=3009 react-scripts start",
    "start:staging": "env-cmd .env.staging react-scripts start",
    "start:prod": "env-cmd .env.prod react-scripts start",
    "start:dist": "yarn build && yarn serve",
    "db": "yarn hasura console",
    "test": "react-scripts test",
    "serve": "serve -s build -c serve.json",
    "upgr": "yarn upgrade-interactive --latest",
    "up": "docker-compose up -d",
    "stop": "docker-compose stop",
    "________________ deployment ________________": "",
    "build:staging": "env-cmd .env.staging npm run build && ncp ../devops/_redirects.staging build/_redirects",
    "build:prod": "env-cmd .env.prod npm run build && ncp ../devops/_redirects.prod build/_redirects",
    "deploy:staging": "cd ../api-starter && yarn deploy:staging",
    "migrate:staging": "yarn hasura:staging migrate apply --endpoint https://staging-aol-starter-hasura.herokuapp.com/",
    "________________ implementation details ________________": "",
    "build": "react-scripts build",
    "hasura": "env-cmd .env hasura --project hasura",
    "hasura:staging": "env-cmd hasura/staging/.env hasura --project hasura",
    "build-snapshot": "yarn build && react-snapshot",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@date-io/date-fns": "2.5.0",
    "@material-ui/core": "4.9.7",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@material-ui/pickers": "^3.2.10",
    "@types/react-datepicker": "^2.11.0",
    "@types/react-select": "^3.0.11",
    "apollo-cache": "^1.3.4",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-retry": "^2.2.15",
    "apollo-link-ws": "^1.0.19",
    "apollo-utilities": "^1.3.3",
    "axios": "0.19.2",
    "formik": "^2.1.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "jwt-decode": "^2.2.0",
    "memoize-one": "^5.1.1",
    "react": "16.13.1",
    "react-apollo": "^3.1.3",
    "react-dom": "16.13.1",
    "react-google-login": "^5.1.2",
    "react-redux": "^7.2.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.4",
    "subscriptions-transport-ws": "^0.9.16",
    "yup": "^0.28.3",
    "zen-observable-ts": "^0.8.20"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@testing-library/dom": "^7.1.2",
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/react": "10.0.1",
    "@testing-library/user-event": "10.0.0",
    "@types/history": "4.7.5",
    "@types/jest": "25.1.4",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "13.5.0",
    "@types/react": "16.9.26",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@types/redux-immutable": "^4.0.1",
    "@types/webpack-env": "1.15.1",
    "@types/yup": "^0.26.33",
    "cross-env": "^7.0.2",
    "env-cmd": "8.0.2",
    "http-proxy-middleware": "^1.0.3",
    "jest-when": "^2.7.0",
    "ncp": "^2.0.0",
    "prettier": "2.0.2",
    "react-scripts": "3.4.1",
    "serve": "11.3.0",
    "tslint": "6.1.0",
    "tslint-config-blvd": "1.2.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3"
  },
  "homepage": "/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": true
  }
}

{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd .env.dev react-scripts start",
    "start:staging": "env-cmd .env.staging react-scripts start",
    "start:prod": "react-scripts start",
    "build:staging": "env-cmd .env.staging npm run build",

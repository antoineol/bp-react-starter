{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:staging": "env-cmd .env.staging react-scripts start",
    "start:prod": "env-cmd .env.prod react-scripts start",
    "build:staging": "env-cmd .env.staging npm run build",
    "build:prod": "env-cmd .env.prod npm run build",
    "test": "react-scripts test",
    "serve": "serve -s build",
    "upgr": "yarn upgrade-interactive --latest",

    "history": "4.10.1",
    "immutable": "4.0.0-rc.12",
    "jwt-decode": "^2.2.0",
    "react": "16.12.0",
    "react-apollo": "^3.1.3",
    "react-app-polyfill": "1.0.5",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
